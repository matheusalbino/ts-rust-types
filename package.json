{
  "name": "@equal/ts-rust-types",
  "version": "1.0.0",
  "description": "Mimic Rust's types",
  "type": "module",
  "sideEffects": false,
  "types": "./lib/types/index.d.ts",
  "module": "./lib/esm/index.js",
